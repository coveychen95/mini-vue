#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sanchen/Common/app-github/mini-vue/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/node_modules:/Users/sanchen/Common/app-github/mini-vue/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/node_modules:/Users/sanchen/Common/app-github/mini-vue/node_modules/.pnpm/typescript@3.9.10/node_modules:/Users/sanchen/Common/app-github/mini-vue/node_modules/.pnpm/node_modules:/Users/sanchen/Common/app-github/mini-vue/node_modules:/Users/sanchen/Common/app-github/node_modules:/Users/sanchen/Common/node_modules:/Users/sanchen/node_modules:/Users/node_modules:/node_modules:/Users/sanchen/Common/app-github/mini-vue/packages/mini-vue/node_modules:/Users/sanchen/Common/app-github/mini-vue/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sanchen/Common/app-github/mini-vue/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/node_modules:/Users/sanchen/Common/app-github/mini-vue/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/node_modules:/Users/sanchen/Common/app-github/mini-vue/node_modules/.pnpm/typescript@3.9.10/node_modules:/Users/sanchen/Common/app-github/mini-vue/node_modules/.pnpm/node_modules:/Users/sanchen/Common/app-github/mini-vue/node_modules:/Users/sanchen/Common/app-github/node_modules:/Users/sanchen/Common/node_modules:/Users/sanchen/node_modules:/Users/node_modules:/node_modules:/Users/sanchen/Common/app-github/mini-vue/packages/mini-vue/node_modules:/Users/sanchen/Common/app-github/mini-vue/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/tsserver" "$@"
fi
